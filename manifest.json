{
    "manifest_version": 2,
    "name": "Reload The Browser",
    "description": "Time Crisis 2 Refresh the page",
    "version": "1.1",
    "icons": {
        "48": "icon.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "contextMenus",
        "webNavigation"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      }
}